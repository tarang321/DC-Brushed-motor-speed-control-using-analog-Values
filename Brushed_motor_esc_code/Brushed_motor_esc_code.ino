
//////// DC motor speed control //


void setup() {
    
}

void loop() {
    
    analogWrite(11,00);
    delay(6000);
    
    analogWrite(11,25);
    delay(4000);
    
    analogWrite(11,50);
    delay(4000);
    
    analogWrite(11,75);
    delay(4000);
    
    analogWrite(11,100);
    delay(4000);
    
    analogWrite(11,125);
    delay(4000);
    
    analogWrite(11,150);
    delay(4000);
    
    analogWrite(11,175);
    delay(4000);
    
    analogWrite(11,200);
    delay(4000);
    
    analogWrite(11,225);
    delay(4000);
    
    analogWrite(11,255);
    delay(7000);
    
    analogWrite(11,0);
    delay(2000);
    
}
